<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="org.model.ZybEntity" table="zyb" schema="stuSys">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="zym">
            <column name="zym" sql-type="char(12)" length="12"/>
        </property>
        <property name="rs">
            <column name="rs" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="fdy">
            <column name="fdy" sql-type="char(8)" length="8" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>